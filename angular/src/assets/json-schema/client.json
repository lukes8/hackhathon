              "formlyConfig": {
                "type": "radio",
                "templateOptions": {
                  "options": [
                    {
                      "value": "HIGH",
                      "label": "vysoký"
                    },
                    {
                      "value": "NORMAL",
                      "label": "normálni"
                    },
                    {
                      "value": "LOW",
                      "label": "nízký"
                    }
                  ]
                }
              }
            }
          },
          "heredity": {
            "type": "textarea",
