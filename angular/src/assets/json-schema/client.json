              "formlyConfig": {
                "className": "custom-short-input"
              }
            }
          },
          "leftOrRightHanded": {
            "type": "string",
            "title": "Levák nebo pravák",
            "widget": {
              "formlyConfig": {
                "type": "radio",
                "templateOptions": {
                  "options": [
                    {
                      "value": true,
                      "label": "Levák"
                    },
                    {
                      "value": false,
                      "label": "Pravák"
                    }
                  ]
                }
              }
            }
          },
          "job": {
            "type": "string",
            "title": "Zaměstnání",
            "widget": {
              "formlyConfig": {
                "type": "radio",
                "templateOptions": {
                  "options": [
                    {
                      "value": "SITTING_AT_PC",
                      "label": "sedavé (u počítače)"
                    },
                    {
                      "value": "SITTING_WHILE_DRIVING",
                      "label": "sedavé ( hodně řídím)"
                    },
                    {
                      "value": "STANDING_WHILE_WORKING",
                      "label": "v práci celý den stojím"
                    },
                    {
                      "value": "MANUAL_WORK",
                      "label": "pracuji fyzicky"
                    },
                    {
                      "value": "RUNNING_SITTING_WALKING",
                      "label": "pobíhám, střídám sed a chůzi"
                    }
